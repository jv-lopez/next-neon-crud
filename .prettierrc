{
  "singleQuote": true,
  "semi": false,
  "trailingComma": "all",
  "jsxSingleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "arrowParens": "always",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "useTabs": false,
  "import/order": [
    "builtin",
    "external",
    "internal",
    "parent",
    "sibling",
    "index"
  ]
}